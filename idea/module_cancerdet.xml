<?xml version="1.0" encoding="UTF-8"?>
<project name="module_cancerdet" default="compile.module.cancerdet">
  <dirname property="module.cancerdet.basedir" file="${ant.file.module_cancerdet}"/>
  
  <property name="module.jdk.home.cancerdet" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.cancerdet" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.cancerdet" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.cancerdet" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="cancerdet.output.dir" value="${module.cancerdet.basedir}/../bin"/>
  <property name="cancerdet.testoutput.dir" value="${module.cancerdet.basedir}/../bin"/>
  
  <path id="cancerdet.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="cancerdet.module.production.classpath">
    <path refid="${module.jdk.classpath.cancerdet}"/>
    <pathelement location="${idea.home}/lib/junit-4.11.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/mockito-all-1.9.5.jar"/>
    <pathelement location="${basedir}/../forms-1.3.0.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/jfeaturelib/JFeatureLib-1.6.0.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/jfeaturelib/JFeatureLib-1.6.0-jar-with-dependencies.jar"/>
    <pathelement location="${basedir}/../resources/liblinear-1.94.jar"/>
    <pathelement location="${basedir}/../../../../../Facultate/An1/Sem2/IntelligentAgents/jade/jade/lib/jade.jar"/>
  </path>
  
  <path id="cancerdet.runtime.production.module.classpath">
    <pathelement location="${cancerdet.output.dir}"/>
    <pathelement location="${idea.home}/lib/junit-4.11.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/mockito-all-1.9.5.jar"/>
    <pathelement location="${basedir}/../forms-1.3.0.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/jfeaturelib/JFeatureLib-1.6.0.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/jfeaturelib/JFeatureLib-1.6.0-jar-with-dependencies.jar"/>
    <pathelement location="${basedir}/../resources/liblinear-1.94.jar"/>
    <pathelement location="${basedir}/../../../../../Facultate/An1/Sem2/IntelligentAgents/jade/jade/lib/jade.jar"/>
  </path>
  
  <path id="cancerdet.module.classpath">
    <pathelement location="${cancerdet.output.dir}"/>
    <path refid="${module.jdk.classpath.cancerdet}"/>
    <pathelement location="${idea.home}/lib/junit-4.11.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/mockito-all-1.9.5.jar"/>
    <pathelement location="${basedir}/../forms-1.3.0.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/jfeaturelib/JFeatureLib-1.6.0.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/jfeaturelib/JFeatureLib-1.6.0-jar-with-dependencies.jar"/>
    <pathelement location="${basedir}/../resources/liblinear-1.94.jar"/>
    <pathelement location="${basedir}/../../../../../Facultate/An1/Sem2/IntelligentAgents/jade/jade/lib/jade.jar"/>
  </path>
  
  <path id="cancerdet.runtime.module.classpath">
    <pathelement location="${cancerdet.output.dir}"/>
    <pathelement location="${idea.home}/lib/junit-4.11.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/mockito-all-1.9.5.jar"/>
    <pathelement location="${basedir}/../forms-1.3.0.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/jfeaturelib/JFeatureLib-1.6.0.jar"/>
    <pathelement location="${basedir}/../../../../../Licenta/workspace/CancerDetection/resources/jfeaturelib/JFeatureLib-1.6.0-jar-with-dependencies.jar"/>
    <pathelement location="${basedir}/../resources/liblinear-1.94.jar"/>
    <pathelement location="${basedir}/../../../../../Facultate/An1/Sem2/IntelligentAgents/jade/jade/lib/jade.jar"/>
  </path>
  
  
  <patternset id="excluded.from.module.cancerdet">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.cancerdet">
    <patternset refid="excluded.from.module.cancerdet"/>
  </patternset>
  
  <path id="cancerdet.module.sourcepath">
    <dirset dir="${module.cancerdet.basedir}/../../BreastCancerDetection">
      <include name="src/main"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.cancerdet" depends="compile.module.cancerdet.production,compile.module.cancerdet.tests" description="Compile module CancerDet"/>
  
  <target name="compile.module.cancerdet.production" depends="register.custom.compilers" description="Compile module CancerDet; production classes">
    <mkdir dir="${cancerdet.output.dir}"/>
    <javac2 destdir="${cancerdet.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.cancerdet}/javac">
      <compilerarg line="${compiler.args.cancerdet}"/>
      <bootclasspath refid="cancerdet.module.bootclasspath"/>
      <classpath refid="cancerdet.module.production.classpath"/>
      <src refid="cancerdet.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.cancerdet"/>
    </javac2>
    
    <copy todir="${cancerdet.output.dir}">
      <fileset dir="${module.cancerdet.basedir}/../src/main">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.cancerdet.tests" depends="register.custom.compilers,compile.module.cancerdet.production" description="compile module CancerDet; test classes" unless="skip.tests"/>
  
  <target name="clean.module.cancerdet" description="cleanup module">
    <delete dir="${cancerdet.output.dir}"/>
    <delete dir="${cancerdet.testoutput.dir}"/>
  </target>
</project>